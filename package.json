{
  "name": "pixly",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x 'tsx --tsconfig ./src/server/tsconfig.json' src/server/main.ts",
    "start": "vite --open",
    "start:server": "NODE_ENV=production tsx --tsconfig ./src/server/tsconfig.json src/server/main.ts",
    "typeorm": "./node_modules/.bin/typeorm",
    "typeorm:sync": "./node_modules/.bin/tsx ./node_modules/typeorm/cli.js schema:sync -d src/server/db/data-source.ts",
    "build": "vite build",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "typecheck": "tsc -p src/client/tsconfig.json --noEmit && tsc -p src/server/tsconfig.json --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@types/bootstrap": "^5.2.10",
    "@types/exif": "^0.6.5",
    "@types/jsonschema": "^1.1.1",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.11.6",
    "@types/reactstrap": "^8.7.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "exif": "^0.6.0",
    "express": "^4.18.2",
    "jsonschema": "^1.4.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-picker-editor": "^1.3.4",
    "react-loading": "^2.0.3",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.23.1",
    "reactstrap": "^9.2.2",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.15.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.2",
    "vite-express": "^0.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.0"
  }
}
